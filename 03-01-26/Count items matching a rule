class Solution:
    def countMatches(self, items, ruleKey, ruleValue):
        if ruleKey == "type":
            x = 0
        elif ruleKey == "color":
            x = 1
        else:
            x = 2
        count = 0
        for i in items:
            if i[x] == ruleValue:
                count += 1
        return count
